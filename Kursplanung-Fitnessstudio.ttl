@prefix schema: <https://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vidp: <https://bmake.th-brandenburg.de/vidp#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix fitness: <http://th-brandenburg.de/ns/kursplanung-fitnessstudio#> .
	

### Definition of Re-Used Classes ### 
schema:Event a owl:Class ;
			rdfs:label "Event"@en .
			
			
schema:SportsEvent a owl:Class ;
			rdfs:label "SportsEvent"@en ;
			rdfs:subClassOf schema:Event .
			
								
schema:Person a owl:Class ;
			rdfs:label "Person"@en .
			

schema:Place a owl:Class ;
			rdfs:label "Place"@en .
			
			
schema:Enumeration a owl:Class ;
			rdfs:label "Enumeration"@en .
			
			
schema:Duration a owl:Class ;
			rdfs:label "Duration"@en .
			
			
schema:Text a rdfs:Class ;
			rdfs:label "Text"@en .
			
			
schema:Date a rdfs:Class ;
			rdfs:label "Date"@en .
			

### Definition of specific Classes ### 
fitness:Participant a owl:Class ;
			rdfs:label "Participant"@en ;
			rdfs:subClassOf schema:Person .

fitness:Trainer a owl:Class ;
			rdfs:label "Trainer"@en ;
			rdfs:subClassOf schema:Person .
			
fitness:Expertise a owl:Class ;
			rdfs:label "Expertise"@en ;
			rdfs:subClassOf schema:Enumeration .
			
### FITNESS TRAINER ###
fitness:CCananau a schema:Person, fitness:Trainer, fitness:Expertise ;
	schema:gender "male" ;
	schema:name "Cristian Cananau" ;
	fitness:areaOfExpertise "Fitnessbombe", "MMA-Trainer", "Frauen-Yoga-Trainer" .
	
fitness:DSchulz a schema:Person, fitness:Trainer, fitness:Expertise ;
	schema:gender "male" ;
	schema:name "Dennis Shulz" ;
	fitness:areaOfExpertise "Fitnesstrainer", "Athletic", "Schwimminstruktor".
	
fitness:JShyshlin a schema:Person, fitness:Trainer, fitness:Expertise ;
	schema:gender "male" ;
	schema:name "Juri Shyshlin" ;
	fitness:areaOfExpertise "Fitnesstrainer", "Jiu-Jitsu-Trainer" .
	
###FITNESS PARTICIPANT###
	
fitness:Participat1 a schema:Person, fitness:Participant, fitness:Expertise ;
	schema:gender "male" ;
	fitness:trainingLevel "Beginner" .

fitness:Participat2 a schema:Person, fitness:Participant, fitness:Expertise ;
	schema:gender "female" ;
	fitness:trainingLevel "Advanced" .

fitness:Participat3 a schema:Person, fitness:Participant, fitness:Expertise ;
	schema:gender "female" ;
	fitness:trainingLevel "Beginner" .
	
###PLACE###
fitness:Raum1 a schema:Place ;
	schema:name "Yogaraum" ;
	fitness:gymEquipment "Sportmatte" .
	
fitness:Raum2 a schema:Place ;
	schema:name "Fitnessraum" ;
	fitness:gymEquipment "Gymkleidungen" .

fitness:Raum3 a schema:Place ;
	schema:name "Rennbahnraum" ;
	fitness:gymEquipment "Sportschue" .
	
###SPORTEVENT###
fitness:YogaTag a schema:SportsEvent, schema:Place, fitness:Trainer, fitness:Participant ;
	schema:location fitness:Raum1 ;
	schema:name "Yoga Tai Chi" ;
	schema:startDate "01.02.2019T15:00:00";
	schema:participant fitness:Participat3 ;
	schema:organizer fitness:CCananau ;
	schema:endDate "01.02.2019T17:00:00" ;
	schema:duration "2 Stunden" .
	
### Object properties ###
schema:participant a owl:ObjectProperty ;
			rdfs:label "participant"@en ;
			rdfs:domain schema:SportsEvent ;
			rdfs:range fitness:Participant .

schema:organizer a owl:ObjectProperty ;
			rdfs:label "organizer"@en ;
			rdfs:domain schema:SportsEvent ;
			rdfs:range fitness:Trainer .


schema:location a owl:ObjectProperty ;
			rdfs:label "location"@en ;
			rdfs:domain schema:SportsEvent ;
			rdfs:range schema:Place .

fitness:areaOfExpertise a owl:ObjectProperty ;
			rdfs:label "area of expertise"@en ;
			rdfs:domain fitness:Trainer ;
			rdfs:range fitness:Expertise .
						
### Datatype Properties ###
schema:name a owl:DatatypeProperty ;
			rdfs:label "name"@en ;
			rdfs:domain [owl:unionOf (schema:Place schema:SportsEvent fitness:Trainer)];
			rdfs:range schema:Text .
			
schema:startDate a owl:DatatypeProperty ;
			rdfs:label "startDate"@en ;
			rdfs:domain schema:SportsEvent ;
			rdfs:range schema:Date .

schema:endDate a owl:DatatypeProperty ;
			rdfs:label "endDate"@en ;
			rdfs:domain schema:SportsEvent ;
			rdfs:range schema:Date .

schema:duration a owl:DatatypeProperty ;
			rdfs:label "duration"@en ;
			rdfs:domain schema:SportsEvent ;
			rdfs:range schema:Duration .

fitness:gymEquipment a owl:DatatypeProperty ;
			rdfs:label "gymEquipment"@en ;
			rdfs:domain schema:Place ;
			rdfs:range schema:Text .

schema:gender a owl:DatatypeProperty ;
			rdfs:label "gender"@en ;
			rdfs:domain schema:Person ;
			rdfs:range schema:Text .

fitness:trainingLevel a owl:DatatypeProperty ;
			rdfs:label "trainingLevel"@en ;
			rdfs:domain fitness:Participant ;
			rdfs:range schema:Text .
