	schema:name "Cristian Cananau" ;
	fitness:areaOfExpertise  fitness:Fitnessbombe,  fitness:MMA, fitness:Frauen-Yoga .
	
fitness:DSchulz a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Dennis Shulz" ;
	fitness:areaOfExpertise  fitness:Fitnesstrainer,  fitness:Athletic, fitness:Schwimminstruktor .

	
fitness:JShyshlin a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Juri Shyshlin" ;
	fitness:areaOfExpertise  fitness:Fitnesstrainer,  fitness:MMA, fitness:Jiu-Jitsu .
	
