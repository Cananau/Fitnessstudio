### Definition of Re-Used Classes ### 
schema:Event a owl:Class ;
			rdfs:label "Event"@en .
			
			
schema:SportsEvent a owl:Class ;
			rdfs:label "SportsEvent"@en ;
			rdfs:subClassOf schema:Event .
			
								
schema:Person a owl:Class ;
			rdfs:label "Person"@en .
			

schema:Place a owl:Class ;
			rdfs:label "Place"@en .
			
			
schema:Enumeration a owl:Class ;
			rdfs:label "Enumeration"@en .			
			
schema:Text a rdfs:Class ;
			rdfs:label "Text"@en .

schema:DayOfWeek a rdfs:Class ;
			rdfs:label "DayOfWeek"@en .

schema:Time a rdfs:Class ;
			rdfs:label "Time"@en .


### Definition of specific Classes ### 
fitness:Participant a owl:Class ;
			rdfs:label "Participant"@en ;
			rdfs:subClassOf schema:Person .

fitness:Trainer a owl:Class ;
			rdfs:label "Trainer"@en ;
			rdfs:subClassOf schema:Person .
			
fitness:Expertise a owl:Class ;
			rdfs:label "Expertise"@en ;
			rdfs:subClassOf schema:Enumeration .
			

### Definition of Expertise ###
fitness:Kampfsport rdf:type owl:NamedIndividual ,
			fitness:Expertise;
			rdfs:comment "Kenntnisse über umfängliche Techniken verschiedener Kampfsportarten und -künste.";
			rdfs:label "Kampfsport"@de .
			
fitness:MMA rdf:type owl:NamedIndividual ,
			fitness:Expertise;
			rdfs:comment "Kenntnisse sowohl über Bodenkampf, als auch Boxen, Kickboxen und Thaiboxen.";
			rdfs:label "MMA-Trainer"@en .

fitness:Frauen-Yoga rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über die philosophische Lehre und über zugehörige passende Übungen.";
			rdfs:label "Frauen-Yoga"@de .


fitness:Muskelaufbau rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über die Anatomie des meschlichen Körpers sowie den gezielten Muskelaufbau durch Schaffung entsprechender Reize.";
			rdfs:label "Muskelaufbau"@de .

fitness:Tanz rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über bekannte Techniken, sowie deren Anwendung anhand von Beispielchoreographien.";
			rdfs:label "Tanz"@de .
			
fitness:Gymnastik rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über gängige Gynastikübungen inklusive zugehöriger Warmup- und Cooldownübungen.";
			rdfs:label "Gymnastik"@de .
			
fitness:Kondition rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse verschiedener Methoden zur Steigerung der Kondition, sowohl mit dem eigenen Körper und mit zugelassenen Geräten.";
			rdfs:label "Kondition"@de .

fitness:Schwimminstruktor rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Ausbildung als Rettungsschwimmer, sowie Kenntnisse über Übungen im Wasser.";
			rdfs:label "Schwimminstruktor"@de .
			
fitness:Biking rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über den sicheren Einsatz verschiedener Zweirad-/ Fahrradmodelle für Trainingszwecke.";
			rdfs:label "Biking"@en .
			
fitness:Ballsport rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über Techniken und Regeln über in Deutschland populäre Ballsportarten.";
			rdfs:label "Ballsport"@de .
			
fitness:Entspannung rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über Entspannungstechniken mit zugelassenen Hilfsmitteln.";
			rdfs:label "Entspannung"@de .
			
fitness:Stretching rdf:type owl:NamedIndividual ,
			fitness:Expertise ;
			rdfs:comment "Kenntnisse über verschiedenste Dehnübung zur begleitung verschiedenster Primärsportarten.";
			rdfs:label "Stretching"@en .

			
### FITNESS TRAINER ###
fitness:CristianCananau a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Cristian Cananau" ;
	rdfs:comment "Cristian hat bereits in der Vergangenheit berufs- und studiumsbegleitend Erfahrung im Sportbereich machen können und ist seit 2016 zugelassener Trainer auf den jeweiligen Fachgebieten.";
	fitness:areaOfExpertise  fitness:Kampfsport, fitness:Frauen-Yoga,  fitness:Muskelaufbau .

fitness:JenniferFerle a fitness:Trainer ;
	schema:gender "female" ;
	schema:name "Jennifer Ferle" ;
	rdfs:comment "Jennifer tanzt bereits seit ihrer Kindheit und übt ihre sportliche Leidenschaft seit letztem Jahr als vollausgebildete Trainerin aus.";
	fitness:areaOfExpertise  fitness:Tanz, fitness:Gymnastik,  fitness:Kondition .
	
fitness:JuriShyshlin a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Juri Shyshlin" ;
	rdfs:comment "Juri hat seinen sportlichen Einstieg klassisch durch das morgentliche Joggen gefunden und über die Jahre eine Menge Kondition aufbauen können. Seit 2015 ist er befähigt, Fitnesskurse geben zu dürfen.";
	fitness:areaOfExpertise fitness:Kondition, fitness:Muskelaufbau .
	
fitness:DennisSchulz a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Dennis Schulz" ;
	rdfs:comment "Dennis war schon immer ein Wasserkind und ist vom Rettungsschwimmer zum Fitnesstrainer auf seinen jeweiligen Fachbereichen gewechselt.";
	fitness:areaOfExpertise fitness:Schwimminstruktor, fitness:Muskelaufbau .
	
fitness:MaxMustermann a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Max Mustermann" ;
	rdfs:comment "Max fährt gerne Fahrrad und hat sich aufgrund der Liebe zu seinem Hobby vor vier Jahren zum Fitnesstrainer ausbilden lassen.";
	fitness:areaOfExpertise fitness:Biking, fitness:Kondition, fitness:Muskelaufbau .

fitness:DirkKeller a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Dirk Keller" ;
	rdfs:comment "Dirk liebt Ballsport, hat eine Amateurkarriere im Basketball hinter sich und gibt seit sieben Jahren entsprechende Fitnesskurse.";
	fitness:areaOfExpertise fitness:Ballsport, fitness:Muskelaufbau, fitness:Kondition .

fitness:MarkBlume a fitness:Trainer ;
	schema:gender "male" ;
	schema:name "Mark Blume" ;
	rdfs:comment "Mark hält gerne seinen Körper gesund und setzt auf das Motto #KlasseStattMasse. Deshalb ist er primär für die Entspannung nach dem Training und das Stretching zuständig.";
	fitness:areaOfExpertise fitness:Entspannung, fitness:Stretching, fitness:Muskelaufbau .
	

