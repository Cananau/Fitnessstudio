@prefix schema: <https://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vidp: <https://bmake.th-brandenburg.de/vidp#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix wd: <http://www.wikidata.org/entity/> .
@prefix fitness: <http://th-brandenburg.de/ns/kursplanung-fitnessstudio#> .
	

### Definition of Re-Used Classes ### 
schema:Event a owl:Class ;
			rdfs:label "Event"@en .
			
			
schema:SportsEvent a owl:Class ;
			rdfs:label "SportsEvent"@en ;
			rdfs:subClassOf schema:Event .
			
								
schema:Person a owl:Class ;
			rdfs:label "Person"@en .
			

schema:Place a owl:Class ;
			rdfs:label "Place"@en .
			
			
schema:Enumeration a owl:Class ;
			rdfs:label "Enumeration"@en .
			
			
schema:Duration a owl:Class ;
			rdfs:label "Duration"@en .
			
			
schema:Text a rdfs:Class ;
			rdfs:label "Text"@en .
			
			
schema:Date a rdfs:Class ;
			rdfs:label "Date"@en .

schema:Expertise a rdfs:Class;
			rdfs:label "Expertise"@en;
			rdfs:subClassOf schema:Enumeration .

### Definition of Expertise ###
fitness:Fitnessbombe rdf:type owl:NamedIndividual ,
			fitness:Expertise;
			rdfs:label "Fitnessbombe"@en .
fitness:MMA rdf:type owl:NamedIndividual ,
			fitness:Expertise;
			rdfs:label "MMA-Trainer"@en .

fitness:Frauen-Yoga a owl:Class ;
			rdfs:label "Frauen-Yoga"@en ;
			rdfs:subClassOf schema:Person .

fitness:Fitnesstrainer a owl:Class ;
			rdfs:label "Fitnesstrainer"@en ;
			rdfs:subClassOf schema:Person .

fitness:Athletic a owl:Class ;
			rdfs:label "Athletic"@en ;
			rdfs:subClassOf schema:Person .

fitness:Schwimminstruktor a owl:Class ;
			rdfs:label "Schwimminstruktor"@en ;
			rdfs:subClassOf schema:Person .

fitness:Jiu-Jitsu a owl:Class ;
			rdfs:label "Jiu-Jitsu"@en ;
			rdfs:subClassOf schema:Person .

### Definition of specific Classes ### 
fitness:Participant a owl:Class ;
			rdfs:label "Participant"@en ;
			rdfs:subClassOf schema:Person .

fitness:Trainer a owl:Class ;
			rdfs:label "Trainer"@en ;
			rdfs:subClassOf schema:Person .
			
fitness:Expertise a owl:Class ;
			rdfs:label "Expertise"@en ;
			rdfs:subClassOf schema:Enumeration .
			
### FITNESS TRAINER ###
fitness:CCananau a fitness:Trainer ;
